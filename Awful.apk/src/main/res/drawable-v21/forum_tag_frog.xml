<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/square_forum_tag_background">
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <size
                android:width="48dp"
                android:height="48dp"/>
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:id="@+id/square_forum_tag_frog"
        android:bottom="5dp"
        android:drawable="@drawable/frog_silhouette"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
    </item>
</layer-list>